  "scripts": {
    "prod": "npm run wipe && npm run prod:build && ./node_modules/.bin/nodemon build",
    "prod:build": "./node_modules/.bin/babel -d ./build ./server -s && ./node_modules/.bin/webpack -p --progress",
    "dev": "npm run wipe && ./node_modules/.bin/concurrently -r --kill-others \"npm run dev:build\" \"npm run dev:wds\"",
    "dev:build": "./node_modules/.bin/nodemon -e js,hbs --ignore node_modules --ignore client/js --ignore client/scss --exec ./node_modules/.bin/babel-node server",
    "dev:wds": "./node_modules/.bin/webpack-dev-server --progress",
    "wipe": "./node_modules/.bin/rimraf dist/js dist/css dist/fonts build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
